<Window
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:classes="clr-namespace:zxeltor.StoCombatAnalyzer.Interface.Classes"
    xmlns:modelcombatlog="clr-namespace:zxeltor.StoCombatAnalyzer.Interface.Model.CombatLog"
    xmlns:controls="clr-namespace:zxeltor.StoCombatAnalyzer.Interface.Controls"
    xmlns:scottPlot="clr-namespace:ScottPlot.WPF;assembly=ScottPlot.WPF"
    x:Class="zxeltor.StoCombatAnalyzer.Interface.MainWindow"
    d:DataContext="{d:DesignInstance Type={x:Type classes:CombatLogManager}}"
    mc:Ignorable="d"
    Title="" Height="600" Width="1000" WindowStartupLocation="CenterScreen">
    <Grid x:Name="uiGridMain">
        <Grid.Resources>
            <classes:DateTimeToExtendedDateStringConverter x:Key="dateTimeToExtendedDateStringConverter" />
            <classes:TypeToVisibilityConverter x:Key="typeToVisibilityConverter" />
            <DataTemplate x:Key="combatListItemTemplate" DataType="{x:Type modelcombatlog:Combat}">
                <Border BorderBrush="Black" BorderThickness="2" Padding="4" Margin="0">
                    <StackPanel Margin="0">
                        <StackPanel Orientation="Horizontal" HorizontalAlignment="Left" VerticalAlignment="Center">
                            <TextBlock Text="Started: " />
                            <TextBlock Text="{Binding CombatStart}" />
                        </StackPanel>
                        <StackPanel Orientation="Horizontal" HorizontalAlignment="Left" VerticalAlignment="Center">
                            <TextBlock Text="Duration: " />
                            <TextBlock Text="{Binding Duration}" />
                        </StackPanel>
                        <StackPanel Orientation="Horizontal" HorizontalAlignment="Left" VerticalAlignment="Center">
                            <TextBlock Text="Players: " />
                            <TextBlock Text="{Binding PlayerEntities.Count}" />
                            <TextBlock Text=", NonPlayers: " />
                            <TextBlock Text="{Binding NonPlayerEntities.Count}" />
                        </StackPanel>
                    </StackPanel>
                </Border>
            </DataTemplate>
            <DataTemplate x:Key="combatEntityItemTemplate" DataType="{x:Type modelcombatlog:CombatEntity}">
                <Border BorderBrush="Black" BorderThickness="2" Padding="4" Margin="0">
                    <StackPanel>
                        <StackPanel Orientation="Horizontal">
                            <TextBlock Text="Name=" />
                            <TextBlock Text="{Binding OwnerDisplay}" />
                        </StackPanel>
                        <StackPanel Orientation="Horizontal">
                            <TextBlock Text="GameId=" />
                            <TextBlock Text="{Binding OwnerInternal}" />
                        </StackPanel>
                        <StackPanel Orientation="Horizontal">
                            <TextBlock Text="Active=" />
                            <TextBlock Text="{Binding Duration}" />
                        </StackPanel>
                        <StackPanel Orientation="Horizontal">
                            <TextBlock Text="Start=" />
                            <TextBlock
                                Text="{Binding EntityStart, Converter={StaticResource dateTimeToExtendedDateStringConverter}}" />
                            <TextBlock Text="End=" Margin="8,0,0,0" />
                            <TextBlock
                                Text="{Binding EntityEnd, Converter={StaticResource dateTimeToExtendedDateStringConverter}}" />
                        </StackPanel>
                        <StackPanel Orientation="Horizontal">
                            <TextBlock Text="Combat Events=" />
                            <TextBlock Text="{Binding CombatEventList.Count}" />
                            <TextBlock Text="Kills=" Margin="8,0,0,0" />
                            <TextBlock Text="{Binding Kills}" />
                        </StackPanel>
                        <StackPanel Orientation="Horizontal">
                            <TextBlock Text="DPS=" />
                            <TextBlock Text="{Binding DPS}" />
                            <TextBlock Text="Max Damage=" Margin="8,0,0,0" />
                            <TextBlock Text="{Binding MaxDamage}" />
                        </StackPanel>
                    </StackPanel>
                </Border>
            </DataTemplate>
            <DataTemplate x:Key="selectedCombatDetailsTemplate" DataType="{x:Type modelcombatlog:Combat}">
                <Grid>
                    <StackPanel Margin="0,0,0,0" Height="50" VerticalAlignment="Top">
                        <StackPanel Orientation="Horizontal" HorizontalAlignment="Left" VerticalAlignment="Center">
                            <TextBlock Text="Started: " />
                            <TextBlock
                                Text="{Binding CombatStart, Converter={StaticResource dateTimeToExtendedDateStringConverter}}" />
                        </StackPanel>
                        <StackPanel Orientation="Horizontal" HorizontalAlignment="Left" VerticalAlignment="Center">
                            <TextBlock Text="Ended: " />
                            <TextBlock
                                Text="{Binding CombatEnd, Converter={StaticResource dateTimeToExtendedDateStringConverter}}" />
                        </StackPanel>
                        <StackPanel Orientation="Horizontal" HorizontalAlignment="Left" VerticalAlignment="Center">
                            <TextBlock Text="Duration: " />
                            <TextBlock Text="{Binding Duration}" />
                        </StackPanel>
                        <StackPanel Orientation="Horizontal" HorizontalAlignment="Left" VerticalAlignment="Center">
                            <TextBlock Text="Combat Entities:" />
                            <TextBlock Text="Players" Margin="8,0,0,0"
                                       Visibility="{Binding PlayerEntities, Converter={StaticResource typeToVisibilityConverter}}" />
                            <TextBlock Text="{Binding PlayerEntities.Count}" Margin="2,0,0,0"
                                       Visibility="{Binding PlayerEntities, Converter={StaticResource typeToVisibilityConverter}}" />
                            <TextBlock Text="Non-Players" Margin="8,0,0,0"
                                       Visibility="{Binding NonPlayerEntities, Converter={StaticResource typeToVisibilityConverter}}" />
                            <TextBlock Text="{Binding NonPlayerEntities.Count}" Margin="2,0,0,0"
                                       Visibility="{Binding NonPlayerEntities, Converter={StaticResource typeToVisibilityConverter}}" />
                        </StackPanel>
                    </StackPanel>
                    <GroupBox Header="Combat Entity List" Margin="0,65,0,0">
                        <TreeView x:Name="uiTreeViewCombatEntityList"
                                  SelectedItemChanged="uiTreeViewCombatEntityList_SelectedItemChanged">
                            <TreeViewItem Header="Players" ItemsSource="{Binding PlayerEntities}"
                                          ItemTemplate="{StaticResource combatEntityItemTemplate}"
                                          HorizontalContentAlignment="Stretch" />
                            <TreeViewItem Header="NonPlayers" ItemsSource="{Binding NonPlayerEntities}"
                                          ItemTemplate="{StaticResource combatEntityItemTemplate}"
                                          HorizontalContentAlignment="Stretch" />
                        </TreeView>
                    </GroupBox>
                </Grid>
            </DataTemplate>
        </Grid.Resources>
        <TabControl x:Name="uiTableControlMain" Margin="0">
            <TabItem x:Name="uiTabLogAnalyzer" Header="Log File Analyzer">
                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="79*" />
                        <RowDefinition Height="60*" />
                    </Grid.RowDefinitions>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="33*"></ColumnDefinition>
                        <ColumnDefinition Width="33*"></ColumnDefinition>
                        <ColumnDefinition Width="33*"></ColumnDefinition>
                    </Grid.ColumnDefinitions>

                    <StackPanel Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="3" Orientation="Horizontal">
                        <Button x:Name="uiButtonParseLog" Content="Parse Log(s)" 
                                Width="80" Height="24" Margin="10,0,0,0"
                                Click="uiButtonParseLog_Click" />
                        <TextBlock Text="Combat List" VerticalAlignment="Center"
                                   Margin="10,0,0,0"/>
                        <TextBlock Text=" (" VerticalAlignment="Center" />
                        <TextBlock Text="{Binding Path=Combats.Count}" VerticalAlignment="Center" />
                        <TextBlock Text="):" VerticalAlignment="Center" />
                        <ComboBox Name="uiComboBoxCombats" Text="Help" ItemsSource="{Binding Combats}" IsSynchronizedWithCurrentItem="True" 
                                  Width="350" Height="36" Margin="4,0,0,0">
                            <ComboBox.ItemTemplate>
                                <DataTemplate>
                                    <StackPanel Orientation="Vertical">
                                        <StackPanel Orientation="Horizontal">
                                            <TextBlock Text="Start=" />
                                            <TextBlock Text="{Binding Path=CombatStart}" />
                                            <TextBlock Text="Duration=" Margin="10,0,0,0" />
                                            <TextBlock Text="{Binding Path=Duration}" />
                                        </StackPanel>
                                        <StackPanel Orientation="Horizontal">
                                            <TextBlock Text="Players=" />
                                            <TextBlock Text="{Binding Path=PlayerEntities.Count}" />
                                            <TextBlock Text="NonPlayers=" Margin="10,0,0,0" />
                                            <TextBlock Text="{Binding Path=NonPlayerEntities.Count}" />
                                        </StackPanel>
                                    </StackPanel>
                                </DataTemplate>
                            </ComboBox.ItemTemplate>
                        </ComboBox>
                    </StackPanel>

                    <GridSplitter Grid.Row="1" Grid.Column="0" Background="Gray" Margin="0,8,-4,4" Width="2"
                                  Padding="4,0,4,0"
                                  HorizontalAlignment="Right"  VerticalAlignment="Stretch" />

                    <GroupBox Grid.Row="1" Grid.Column="0" x:Name="uiGroupBoxSelectedCombatDetails" Header="Selected Combat Details"
                              Padding="4,0,4,4">
                        <Grid>
                            <ContentControl Content="{Binding Combats}"
                                            ContentTemplate="{StaticResource selectedCombatDetailsTemplate}" />
                        </Grid>
                    </GroupBox>

                    <GridSplitter Grid.Row="1" Grid.Column="1" Background="Gray" Margin="0,8,-4,4" Width="2"
                                  Padding="4,0,4,0"
                                  HorizontalAlignment="Right"  VerticalAlignment="Stretch" />

                    <GroupBox Grid.Row="1" Grid.Column="1" Header="Selected Combat Entity: Event Types Breakdown" Margin="4,0,0,0">
                        <TreeView>
                            <TreeViewItem Name="uiTreeviewItemEntityTypes" Header="Entity" 
                                          ItemsSource="{Binding Path=SelectedEntityCombatEventTypeList}">
                                <TreeViewItem.ItemTemplate>
                                    <HierarchicalDataTemplate>
                                        <TreeViewItem Header="{Binding Path=EventDisplay}">
                                            <StackPanel Orientation="Horizontal">
                                                <TextBlock Text="DPS=" />
                                                <TextBlock Text="{Binding Path=Dps}" />

                                                <TextBlock Text="MaxMagnitude=" Margin="10,0,0,0" />
                                                <TextBlock Text="{Binding Path=MaxMagnitude}" />

                                                <TextBlock Text="MaxMagnitudeBase=" Margin="10,0,0,0" />
                                                <TextBlock Text="{Binding Path=MaxMagnitudeBase}" />
                                            </StackPanel>
                                            <DataGrid ItemsSource="{Binding Path=CombatEvents}" AutoGenerateColumns="False"
                                                      CanUserReorderColumns="True" CanUserSortColumns="True"
                                                      CanUserResizeColumns="True">
                                                <DataGrid.Columns>
                                                    <DataGridTextColumn Header="Timestamp" Binding="{Binding Path=Timestamp, Mode=OneTime}" IsReadOnly="True" />
                                                    <DataGridTextColumn Header="TargetDisplay" Binding="{Binding Path=TargetDisplay, Mode=OneTime}" IsReadOnly="True" />
                                                    <DataGridTextColumn Header="Type" Binding="{Binding Path=Type, Mode=OneTime}" IsReadOnly="True" />
                                                    <DataGridTextColumn Header="Flags" Binding="{Binding Path=Flags, Mode=OneTime}" IsReadOnly="True" />
                                                    <DataGridTextColumn Header="Magnitude" Binding="{Binding Path=Magnitude, Mode=OneTime}" IsReadOnly="True" />
                                                    <DataGridTextColumn Header="MagnitudeBase" Binding="{Binding Path=MagnitudeBase, Mode=OneTime}" IsReadOnly="True" />
                                                </DataGrid.Columns>
                                            </DataGrid>
                                        </TreeViewItem>
                                    </HierarchicalDataTemplate>
                                </TreeViewItem.ItemTemplate>
                            </TreeViewItem>
                            <TreeViewItem Name="uiTreeviewItemPetTypes" Header="Entity Pet" ItemsSource="{Binding Path=SelectedEntityPetCombatEventTypeList}">
                                <TreeViewItem.ItemTemplate>
                                    <HierarchicalDataTemplate>
                                        <TreeViewItem Header="{Binding Path=SourceDisplay}" ItemsSource="{Binding Path=CombatEventTypes}">
                                            <TreeViewItem.ItemTemplate>
                                                <HierarchicalDataTemplate>
                                                    <TreeViewItem Header="{Binding Path=EventDisplay}">
                                                        <StackPanel Orientation="Horizontal">
                                                            <TextBlock Text="DPS=" />
                                                            <TextBlock Text="{Binding Path=Dps}" />

                                                            <TextBlock Text="MaxMagnitude=" Margin="10,0,0,0" />
                                                            <TextBlock Text="{Binding Path=MaxMagnitude}" />

                                                            <TextBlock Text="MaxMagnitudeBase=" Margin="10,0,0,0" />
                                                            <TextBlock Text="{Binding Path=MaxMagnitudeBase}" />
                                                        </StackPanel>
                                                        <DataGrid ItemsSource="{Binding Path=CombatEvents}" AutoGenerateColumns="False"
                                                                  CanUserReorderColumns="True" CanUserSortColumns="True"
                                                                  CanUserResizeColumns="True">
                                                            <DataGrid.Columns>
                                                                <DataGridTextColumn Header="Timestamp" Binding="{Binding Path=Timestamp, Mode=OneTime}" IsReadOnly="True" />
                                                                <DataGridTextColumn Header="TargetDisplay" Binding="{Binding Path=TargetDisplay, Mode=OneTime}" IsReadOnly="True" />
                                                                <DataGridTextColumn Header="Type" Binding="{Binding Path=Type, Mode=OneTime}" IsReadOnly="True" />
                                                                <DataGridTextColumn Header="Flags" Binding="{Binding Path=Flags, Mode=OneTime}" IsReadOnly="True" />
                                                                <DataGridTextColumn Header="Magnitude" Binding="{Binding Path=Magnitude, Mode=OneTime}" IsReadOnly="True" />
                                                                <DataGridTextColumn Header="MagnitudeBase" Binding="{Binding Path=MagnitudeBase, Mode=OneTime}" IsReadOnly="True" />
                                                            </DataGrid.Columns>
                                                        </DataGrid>
                                                    </TreeViewItem>
                                                </HierarchicalDataTemplate>
                                            </TreeViewItem.ItemTemplate>
                                            <!--<DataGrid ItemsSource="{Binding Path=CombatEventTypes}" AutoGenerateColumns="False"
                                                      CanUserReorderColumns="True" CanUserSortColumns="True"
                                                      CanUserResizeColumns="True">
                                                <DataGrid.Columns>
                                                    <DataGridTextColumn Header="SourceLabel" Binding="{Binding Path=SourceLabel, Mode=OneTime}" IsReadOnly="True" />
                                                    <DataGridTextColumn Header="TargetLabel" Binding="{Binding Path=TargetLabel, Mode=OneTime}" IsReadOnly="True" />
                                                    <DataGridTextColumn Header="Type" Binding="{Binding Path=Type, Mode=OneTime}" IsReadOnly="True" />
                                                    <DataGridTextColumn Header="Flags" Binding="{Binding Path=Flags, Mode=OneTime}" IsReadOnly="True" />
                                                    <DataGridTextColumn Header="Magnitude" Binding="{Binding Path=Magnitude, Mode=OneTime}" IsReadOnly="True" />
                                                    <DataGridTextColumn Header="MagnitudeBase" Binding="{Binding Path=MagnitudeBase, Mode=OneTime}" IsReadOnly="True" />
                                                </DataGrid.Columns>
                                            </DataGrid>-->
                                        </TreeViewItem>
                                    </HierarchicalDataTemplate>
                                </TreeViewItem.ItemTemplate>
                            </TreeViewItem>
                        </TreeView>
                    </GroupBox>

                    <GridSplitter Grid.Row="2" Grid.Column="0" Grid.ColumnSpan="3" Background="Gray" Height="2" Margin="10,4,10,0"
                                  HorizontalAlignment="Stretch" VerticalAlignment="Top" />

                    <GroupBox x:Name="uiGroupBoxSelectedCombatEntity" Grid.Row="1" Grid.Column="2" Margin="10,0,0,0"
                              Padding="0,4,0,0">
                        <GroupBox.Header>
                            <StackPanel Orientation="Horizontal">
                                <TextBlock Text="Selected Combat Entity: All Events" />
                                <TextBlock Text="(" Margin="4,0,0,0" />
                                <TextBlock Text="{Binding SelectedEntityCombatEventList.Count}" />
                                <TextBlock Text=")" />
                            </StackPanel>
                        </GroupBox.Header>
                        <DataGrid Name="uiDataGridAllEvents" ItemsSource="{Binding SelectedEntityCombatEventList}" AutoGenerateColumns="False"
                                  CanUserReorderColumns="True" CanUserSortColumns="True"
                                  CanUserResizeColumns="True">
                            <DataGrid.ContextMenu>
                                <ContextMenu>
                                    <StackPanel Orientation="Vertical">
                                        <TextBlock Text="Select Columns:"></TextBlock>
                                        <CheckBox Content="Filename" IsChecked="{Binding Path=FilenameVisible, Mode=TwoWay}" 
                                                  Tag="Filename" Checked="ToggleButton_OnChecked" Unchecked="ToggleButton_OnUnchecked" />
                                        <CheckBox Content="LineNumber" IsChecked="{Binding Path=LineNumberVisible, Mode=TwoWay}" 
                                                  Tag="LineNumber" Checked="ToggleButton_OnChecked" Unchecked="ToggleButton_OnUnchecked" />
                                        <CheckBox Content="Timestamp" IsChecked="{Binding Path=TimestampVisible, Mode=TwoWay}" 
                                                  Tag="Timestamp" Checked="ToggleButton_OnChecked" Unchecked="ToggleButton_OnUnchecked" />
                                        <CheckBox Content="OwnerDisplay" IsChecked="{Binding Path=OwnerDisplayVisible, Mode=TwoWay}" 
                                                  Tag="OwnerDisplay" Checked="ToggleButton_OnChecked" Unchecked="ToggleButton_OnUnchecked" />
                                        <CheckBox Content="OwnerInternal" IsChecked="{Binding Path=OwnerInternalVisible, Mode=TwoWay}" 
                                                  Tag="OwnerInternal" Checked="ToggleButton_OnChecked" Unchecked="ToggleButton_OnUnchecked" />
                                        <CheckBox Content="SourceDisplay" IsChecked="{Binding Path=SourceDisplayVisible, Mode=TwoWay}" 
                                                  Tag="SourceDisplay" Checked="ToggleButton_OnChecked" Unchecked="ToggleButton_OnUnchecked" />
                                        <CheckBox Content="SourceInternal" IsChecked="{Binding Path=SourceInternalVisible, Mode=TwoWay}" 
                                                  Tag="SourceInternal" Checked="ToggleButton_OnChecked" Unchecked="ToggleButton_OnUnchecked" />
                                        <CheckBox Content="TargetDisplay" IsChecked="{Binding Path=TargetDisplayVisible, Mode=TwoWay}" 
                                                  Tag="TargetDisplay" Checked="ToggleButton_OnChecked" Unchecked="ToggleButton_OnUnchecked" />
                                        <CheckBox Content="TargetInternal" IsChecked="{Binding Path=TargetInternalVisible, Mode=TwoWay}" 
                                                  Tag="TargetInternal" Checked="ToggleButton_OnChecked" Unchecked="ToggleButton_OnUnchecked" />
                                        <CheckBox Content="EventDisplay" IsChecked="{Binding Path=EventDisplayVisible, Mode=TwoWay}" 
                                                  Tag="EventDisplay" Checked="ToggleButton_OnChecked" Unchecked="ToggleButton_OnUnchecked" />
                                        <CheckBox Content="EventInternal" IsChecked="{Binding Path=EventInternalVisible, Mode=TwoWay}" 
                                                  Tag="EventInternal" Checked="ToggleButton_OnChecked" Unchecked="ToggleButton_OnUnchecked" />
                                        <CheckBox Content="Type" IsChecked="{Binding Path=TypeVisible, Mode=TwoWay}" 
                                                  Tag="Type" Checked="ToggleButton_OnChecked" Unchecked="ToggleButton_OnUnchecked" />
                                        <CheckBox Content="Flags" IsChecked="{Binding Path=FlagsVisible, Mode=TwoWay}" 
                                                  Tag="Flags" Checked="ToggleButton_OnChecked" Unchecked="ToggleButton_OnUnchecked" />
                                        <CheckBox Content="Magnitude" IsChecked="{Binding Path=MagnitudeVisible, Mode=TwoWay}" 
                                                  Tag="Magnitude" Checked="ToggleButton_OnChecked" Unchecked="ToggleButton_OnUnchecked" />
                                        <CheckBox Content="MagnitudeBase" IsChecked="{Binding Path=MagnitudeBaseVisible, Mode=TwoWay}" 
                                                  Tag="MagnitudeBase" Checked="ToggleButton_OnChecked" Unchecked="ToggleButton_OnUnchecked" />
                                    </StackPanel>
                                </ContextMenu>
                            </DataGrid.ContextMenu>
                            <DataGrid.Columns>
                                <DataGridTextColumn Header="Filename"
                                                    Binding="{Binding OriginalFileName, Mode=OneTime}"
                                                    IsReadOnly="True" />
                                <DataGridTextColumn Header="LineNumber"
                                                    Binding="{Binding OriginalFileLineNumber, Mode=OneTime}"
                                                    IsReadOnly="True" />
                                <DataGridTextColumn Header="Timestamp"
                                                    Binding="{Binding Timestamp, Converter={StaticResource dateTimeToExtendedDateStringConverter}, Mode=OneTime}"
                                                    IsReadOnly="True" />
                                <DataGridTextColumn Header="OwnerDisplay" Binding="{Binding OwnerDisplay, Mode=OneTime}"
                                                    IsReadOnly="True" />
                                <DataGridTextColumn Header="OwnerInternal" Binding="{Binding OwnerInternal, Mode=OneTime}"
                                                    IsReadOnly="True" />
                                <DataGridTextColumn Header="SourceDisplay" Binding="{Binding SourceDisplay, Mode=OneTime}"
                                                    IsReadOnly="True" />
                                <DataGridTextColumn Header="SourceInternal" Binding="{Binding SourceInternal, Mode=OneTime}"
                                                    IsReadOnly="True" />
                                <DataGridTextColumn Header="TargetDisplay" Binding="{Binding TargetDisplay, Mode=OneTime}"
                                                    IsReadOnly="True" />
                                <DataGridTextColumn Header="TargetInternal" Binding="{Binding TargetInternal, Mode=OneTime}"
                                                    IsReadOnly="True" />
                                <DataGridTextColumn Header="EventDisplay" Binding="{Binding EventDisplay, Mode=OneTime}"
                                                    IsReadOnly="True" />
                                <DataGridTextColumn Header="EventInternal" Binding="{Binding EventInternal, Mode=OneTime}"
                                                    IsReadOnly="True" />
                                <DataGridTextColumn Header="Type" Binding="{Binding Type, Mode=OneTime}"
                                                    IsReadOnly="True" />
                                <DataGridTextColumn Header="Flags" Binding="{Binding Flags, Mode=OneTime}"
                                                    IsReadOnly="True" />
                                <DataGridTextColumn Header="Magnitude" Binding="{Binding Magnitude, Mode=OneTime}"
                                                    IsReadOnly="True" />
                                <DataGridTextColumn Header="MagnitudeBase"
                                                    Binding="{Binding MagnitudeBase, Mode=OneTime}" 
                                                    IsReadOnly="True" />
                            </DataGrid.Columns>
                        </DataGrid>
                    </GroupBox>

                    <GroupBox Name="uiGridBoxEntity" Grid.Row="2" Grid.Column="0" Grid.ColumnSpan="3" Header="Selected Combat Entity: Plot">
                        <scottPlot:WpfPlot Name="uiScottPlotEntity">

                        </scottPlot:WpfPlot>
                    </GroupBox>
                </Grid>
            </TabItem>
            <TabItem Header="Settings">
                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="347*" />
                        <RowDefinition Height="209*" />
                    </Grid.RowDefinitions>
                    <GridSplitter Background="Gray" Height="2" Margin="10,0,10,0" HorizontalAlignment="Stretch"
                                  VerticalAlignment="Top" Grid.Row="1" />

                    <GroupBox Header="Log File Analyzer Settings" Grid.Row="0" Margin="10,10,10,10">
                        <Grid>
                            <controls:SettingsUserControl Margin="0,0,0,0" />
                        </Grid>
                    </GroupBox>

                    <GroupBox Header="Logging Summary" Grid.Row="1" Margin="10,10,10,10">
                        <Grid Margin="0,0,0,0">
                            <TextBox x:Name="uiTextBoxLog" Margin="4,4,4,4" BorderThickness="0"
                                            TextWrapping="NoWrap" Text="" HorizontalScrollBarVisibility="Auto"
                                            VerticalScrollBarVisibility="Auto" IsReadOnly="True"/>
                        </Grid>
                    </GroupBox>
                </Grid>
            </TabItem>
        </TabControl>
    </Grid>
</Window>