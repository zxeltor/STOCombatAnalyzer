                                                                <Image Source="Images/glyphicons-433-plus.png" Height="8" />
                                                            </Button>
                                                            <TextBlock Text="MapEntities" Margin="4,0,0,0"></TextBlock>
                                                        </StackPanel>
                                                    </TreeViewItem.Header>
                                                    <TreeViewItem.ItemTemplate>
                                                        <DataTemplate>
                                                            <StackPanel Orientation="Horizontal">
                                                                <Button Tag="EditMapEntity" Margin="0,0,0,0" ToolTip="Edit pattern" 
                                                Click="MapDetectButton_OnClick" CommandParameter="{Binding}"
                                                VerticalAlignment="Center">
                                                                    <Image Source="Images/glyphicons-151-edit.png" Height="8" />
                                                                </Button>
                                                                <Button Tag="DeleteMapEntity" Margin="4,0,0,0" ToolTip="Delete pattern"
                                                VerticalAlignment="Center"
                                                Click="MapDetectButton_OnClick" CommandParameter="{Binding}">
                                                                    <Image Source="Images/glyphicons-257-delete.png" Height="8" />
                                                                </Button>
                                                                <CheckBox ToolTip="IsUniqueToMap" Margin="4,0,0,0" 
                                                  VerticalAlignment="Center"
                                                  IsChecked="{Binding Path=IsUniqueToMap}"></CheckBox>
                                                                <TextBlock Text="{Binding Path=Pattern}" Margin="4,0,0,0"></TextBlock>
                                                            </StackPanel>
                                                        </DataTemplate>
                                                    </TreeViewItem.ItemTemplate>
                                                </TreeViewItem>
                                                <TreeViewItem Tag="MapEntityExclusion" ItemsSource="{Binding Path=MapEntityExclusionsOrderByPattern}" IsExpanded="True">
                                                    <TreeViewItem.Header>
                                                        <StackPanel Orientation="Horizontal" HorizontalAlignment="Right">
                                                            <Button Tag="AddMapExceptionEntity" Margin="0,0,0,0" ToolTip="Add map entity exclusion"
                                            Click="MapDetectButton_OnClick" CommandParameter="{Binding}">
                                                                <Image Source="Images/glyphicons-433-plus.png" Height="8" />
                                                            </Button>
                                                            <TextBlock Margin="4,0,0,0" Text="MapEntityExclusions"></TextBlock>
                                                        </StackPanel>
                                                    </TreeViewItem.Header>
                                                    <TreeViewItem.ItemTemplate>
                                                        <DataTemplate>
                                                            <StackPanel Orientation="Horizontal">
                                                                <Button Tag="EditMapExceptionEntity" Margin="0,0,0,0" ToolTip="Edit pattern" 
                                                Click="MapDetectButton_OnClick" CommandParameter="{Binding}"
                                                VerticalAlignment="Center">
                                                                    <Image Source="Images/glyphicons-151-edit.png" Height="8" />
                                                                </Button>
                                                                <Button Tag="DeleteMapExceptionEntity" Margin="4,0,0,0" ToolTip="Delete pattern"
                                                VerticalAlignment="Center"
                                                Click="MapDetectButton_OnClick" CommandParameter="{Binding}">
                                                                    <Image Source="Images/glyphicons-257-delete.png" Height="8" />
                                                                </Button>
                                                                <CheckBox ToolTip="IsUniqueToMap" Margin="4,0,0,0" 
                                                  VerticalAlignment="Center"
                                                  IsChecked="{Binding Path=IsUniqueToMap}"></CheckBox>
                                                                <TextBlock Text="{Binding Path=Pattern}" Margin="4,0,0,0"></TextBlock>
                                                            </StackPanel>
                                                        </DataTemplate>
                                                    </TreeViewItem.ItemTemplate>
                                                </TreeViewItem>
                                            </StackPanel>

                                        </TreeViewItem>
                                    </HierarchicalDataTemplate>
                                </TreeView.ItemTemplate>
                            </TreeView>
                        </Grid>
                    </GroupBox>

                    <GroupBox Grid.Row="2" Grid.Column="3">
                        <GroupBox.Header>
                            <StackPanel Orientation="Horizontal">
                                <TextBlock Text="EntityExclusionList" VerticalAlignment="Center"></TextBlock>
                                <Button Tag="AddExceptionEntity" Margin="4,0,0,0" ToolTip="Add a new entity" 
                            Click="MapDetectButton_OnClick"
                            VerticalAlignment="Center">
                                    <Image Source="Images/glyphicons-433-plus.png" Height="8" />
                                </Button>
                            </StackPanel>
                        </GroupBox.Header>
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="*"></RowDefinition>
                            </Grid.RowDefinitions>
                            <ListView Grid.Row="0" ItemsSource="{Binding Path=CombatMapDetectionSettings.EntityExclusionListOrderedByPattern}">
                                <ListView.ItemTemplate>
                                    <DataTemplate>
                                        <StackPanel Orientation="Horizontal">
                                            <Button Tag="EditExceptionEntity" Margin="4,0,0,0" ToolTip="Edit pattern"
                                        Click="MapDetectButton_OnClick" CommandParameter="{Binding}">
                                                <Image Source="Images/glyphicons-151-edit.png" Height="8" />
                                            </Button>
                                            <Button Tag="DeleteExceptionEntity" Margin="4,0,0,0" ToolTip="Delete pattern"
                                        Click="MapDetectButton_OnClick" CommandParameter="{Binding}">
                                                <Image Source="Images/glyphicons-257-delete.png" Height="8" />
                                            </Button>
                                            <TextBlock Text="{Binding Path=Pattern}" Margin="4,0,0,0"></TextBlock>
                                        </StackPanel>
                                    </DataTemplate>
                                </ListView.ItemTemplate>
                            </ListView>
                        </Grid>
                    </GroupBox>
                </Grid>
            </TabItem>

            <TabItem Header="Tools/Settings">
                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="66*" />
                    </Grid.RowDefinitions>
                    <GridSplitter Background="Gray" Height="2" Margin="10,0,10,0" HorizontalAlignment="Stretch"
                                  VerticalAlignment="Top" Grid.Row="1" />

                    <GroupBox Header="Tools" Grid.Row="0" Margin="10,10,10,10" Padding="10,10,10,10">
                        <StackPanel Orientation="Horizontal">
                            <Image Tag="import_detection_json" Source="Images/glyphicons-196-info-sign.png" Stretch="Fill" Height="16" Margin="0,0,0,0"
                                   MouseLeftButtonUp="DetailsImage_OnMouseLeftButtonUp" ToolTip="Click for more details." />
                            <Button Name="uiButtonImportMapEntities" Content="Import Map Detection Settings" VerticalAlignment="Center" Padding="4,4,4,4" Margin="4,0,0,0"
                                    Click="UiButtonImportMapEntities_OnClick"/>

                            <Image Tag="reset_detection_json" Source="Images/glyphicons-196-info-sign.png" Stretch="Fill" Height="16" Margin="16,0,0,0"
                                   MouseLeftButtonUp="DetailsImage_OnMouseLeftButtonUp" ToolTip="Click for more details."/>
                            <Button Name="uiButtonResetMapEntities" Content="Reset Map Detection Settings" VerticalAlignment="Center" Padding="4,4,4,4" Margin="4,0,0,0"
                                    Click="UiButtonResetMapEntities_OnClick"/>

                            <Image Tag="export_combat_json" Source="Images/glyphicons-196-info-sign.png" Stretch="Fill" Height="16" Margin="16,0,0,0"
                                   MouseLeftButtonUp="DetailsImage_OnMouseLeftButtonUp" ToolTip="Click for more details."/>
                            <Button Name="uiButtonExportCombat" Content="Export Selected Combat" VerticalAlignment="Center" Padding="4,4,4,4" Margin="4,0,0,0"
                                    Click="UiButtonExportCombat_OnClick"/>
                        </StackPanel>
                    </GroupBox>

                    <GroupBox Header="Settings" Grid.Row="1" Margin="10,10,10,10">
                        <Grid>
                            <controls:SettingsUserControl Margin="0,0,0,0" VerticalAlignment="Top"/>
                        </Grid>
                    </GroupBox>
                </Grid>
            </TabItem>

            <TabItem Header="About" Height="20" VerticalAlignment="Top">
                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="Auto"></RowDefinition>
                        <RowDefinition Height="*"></RowDefinition>
                    </Grid.RowDefinitions>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="*"></ColumnDefinition>
                        <ColumnDefinition Width="Auto"></ColumnDefinition>
                        <ColumnDefinition Width="*"></ColumnDefinition>
                    </Grid.ColumnDefinitions>

                    <Image Name="uiImageAbout" Grid.Row="0" Grid.Column="0" Margin="10,10,10,10" HorizontalAlignment="Center" VerticalAlignment="Center" Width="128" Height="128"
                           Source="Images/trekwphaser-128x128.png" />

                    <StackPanel Grid.Row="0" Grid.Column="1" Orientation="Vertical" VerticalAlignment="Center"  HorizontalAlignment="Center">
                        <TextBlock Text="{x:Static properties:Resources.ApplicationName}" FontSize="40" FontWeight="Bold"/>
                        <StackPanel Orientation="Horizontal">
                            <TextBlock Text="Version:" FontSize="16" FontWeight="Bold" />
                            <TextBlock Text="{Binding Path=ApplicationVersionInfoString, Mode=OneTime}" FontSize="16" Margin="8,0,0,0" />
                        </StackPanel>
                        <StackPanel Orientation="Horizontal" Margin="0,20,0,0">
                            <TextBlock Text="Map Detection Settings Version:" FontWeight="Bold" />
                            <TextBlock Text="{Binding CombatMapDetectionSettings.JsonVersion}" Margin="8,0,0,0" />
                        </StackPanel>
                    </StackPanel>

                    <Image Name="uiImageAbout2" Grid.Row="0" Grid.Column="2" Margin="10,10,10,10" HorizontalAlignment="Center" VerticalAlignment="Center" Width="128" Height="128"
                           Source="Images/trekwphaser-128x128.png" />

                    <GroupBox Grid.Row="1" Grid.Column="0" Grid.ColumnSpan="3" Margin="10,10,10,10" Padding="10,10,10,10">
                        <Grid HorizontalAlignment="Center" Margin="10,0,0,0">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="Auto" />
                                <ColumnDefinition Width="Auto" />
                                <ColumnDefinition Width="Auto" />
                            </Grid.ColumnDefinitions>

                            <TextBlock Grid.Row="0" Grid.Column="0" Text="Source Code:" FontWeight="Bold" HorizontalAlignment="Right" />
                            <TextBox Grid.Row="0" Grid.Column="1" Text="{x:Static properties:Resources.GithubRepoUrl}" IsReadOnly="True" Margin="8,0,0,0" />
                            <Button Tag="GithubRepoUrl" Grid.Row="0" Grid.Column="2" HorizontalAlignment="Left" Height="20" Margin="8,0,0,0" Click="Browse_OnMouseLeftButtonUp">
                                <Button.ToolTip>Attempt to open URL in default browser.</Button.ToolTip>
                                <Image Source="Images/glyphicons-152-new-window.png" />
                            </Button>

                            <TextBlock Grid.Row="1" Grid.Column="0" Text="Wiki:" Margin="0,10,0,0" FontWeight="Bold" HorizontalAlignment="Right" />
                            <TextBox Grid.Row="1" Grid.Column="1" Text="{x:Static properties:Resources.GithubRepoWikiUrl}" IsReadOnly="True" Margin="8,10,0,0" />
                            <Button Tag="GithubRepoWikiUrl" Grid.Row="1" Grid.Column="2" HorizontalAlignment="Left" Height="20" Margin="8,10,0,0" Click="Browse_OnMouseLeftButtonUp">
                                <Button.ToolTip>Attempt to open URL in default browser.</Button.ToolTip>
                                <Image Source="Images/glyphicons-152-new-window.png" />
                            </Button>

                            <TextBlock Grid.Row="2" Grid.Column="0" Text="Latest Map Detection Settings:" Margin="0,10,0,0" FontWeight="Bold" HorizontalAlignment="Right" />
                            <TextBox Grid.Row="2" Grid.Column="1" Text="{x:Static properties:Resources.GithubMapDetectRepoUrl}" IsReadOnly="True" Margin="8,10,0,0" />
                            <Button Tag="GithubMapDetectRepoUrl" Grid.Row="2" Grid.Column="2" HorizontalAlignment="Left" Height="20" Margin="8,10,0,0" Click="Browse_OnMouseLeftButtonUp">
                                <Button.ToolTip>Attempt to open URL in default browser.</Button.ToolTip>
                                <Image Source="Images/glyphicons-152-new-window.png" />
                            </Button>
                        </Grid>
                    </GroupBox>
                </Grid>
            </TabItem>
        </TabControl>
    </Grid>
</Window>